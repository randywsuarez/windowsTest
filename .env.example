# Ejemplo de variables de entorno
# Reemplaza estos valores con tus propias credenciales

# API Credentials
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here

# Production Environment
PROD_API_URL=https://api.production.com
PROD_EXTERNAL_URL=https://external.production.com
PROD_DB_NAME=prod_db
PROD_UPDATE_ENABLED=true
PROD_UPDATE_URL=https://updates.production.com

# Testing Environment
TEST_API_URL=https://api.testing.com
TEST_EXTERNAL_URL=https://external.testing.com
TEST_DB_NAME=test_db
TEST_UPDATE_ENABLED=true
TEST_UPDATE_URL=https://updates.testing.com

# Development Environment
DEV_API_URL=https://api.dev.com
DEV_EXTERNAL_URL=https://external.dev.com
DEV_DB_NAME=dev_db
DEV_UPDATE_ENABLED=true
DEV_UPDATE_URL=https://updates.dev.com

# Local Environment
LOCAL_API_URL=http://localhost:3000
LOCAL_EXTERNAL_URL=http://localhost:8080
LOCAL_DB_NAME=local_db
LOCAL_UPDATE_ENABLED=false
LOCAL_UPDATE_URL=http://localhost:8000
